{
  "name": "HawkerGo",
  "displayName": "HawkerGo",
  "expo": {
    "name": "HawkerGo",
    "slug": "hawker-go",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#e67e22"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hawkergo.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#e67e22"
      },
      "package": "com.hawkergo.app"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    }
  }
}